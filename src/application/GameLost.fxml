<BorderPane prefHeight="821.0" prefWidth="1115.0" scaleShape="false" style="-fx-background-image: url(&quot;assets\menuassests\Ground_3.png&quot;);" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" BorderPane.alignment="CENTER">
