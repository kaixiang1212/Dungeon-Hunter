               <textFill>
                  <RadialGradient centerX="0.4943820224719101" centerY="0.5144230769230769" focusAngle="168.69" focusDistance="-0.023809523809523836" radius="0.7023809523809523">
                     <stops>
                        <Stop color="#ff2626" />
                        <Stop color="WHITE" offset="1.0" />
                     </stops>
                  </RadialGradient>
               </textFill>
               <effect>
                  <Glow level="0.0" />
               </effect>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Label>
            <Button fx:id="mainMenuButton1" mnemonicParsing="false" onAction="#handleMainMenuButton" prefHeight="35.0" prefWidth="100.0" text="Main Menu" />
